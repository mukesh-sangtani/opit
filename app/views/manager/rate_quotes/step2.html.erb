<script type="text/javascript">
$(function () {
  var isMouseDown = false,
    isHighlighted;
  $("#rate_quote_table td")
    .mousedown(function () {
      isMouseDown = true;
      $(this).toggleClass("highlighted");
      isHighlighted = $(this).hasClass("highlighted");
      return false; // prevent text selection
    })
    .mouseover(function () {
      if (isMouseDown) {
        $(this).toggleClass("highlighted", isHighlighted);
      }
    });

  $(document)
    .mouseup(function () {
      isMouseDown = false;
  });
  
  $("#js_select_all").click(function(){
      $("#rate_quote_table td").removeClass("highlighted");
      $("#rate_quote_table td").addClass("highlighted");
  });
  
  $("#js_deselect_all").click(function(){
      $("#rate_quote_table td").removeClass("highlighted");
  });

  $(".css_elements").click(function(){
      var css_attr = $(this).attr("css_attr");
      var css_val = $(this).attr("css_val");
      var css_default = $(this).attr("css_default");
    $("#rate_quote_table td.highlighted").each(function(a,b){
      if($(this).css(css_attr) == css_val){
        $(this).css(css_attr,css_default);
      }else{
        $(this).css(css_attr,css_val);
      }
    });
  });

});
</script>